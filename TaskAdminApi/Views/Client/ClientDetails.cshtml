@model TaskAdminApi.Models.Client

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Клиент:</th>
            <th>Контактное лицо:</th>
            <th>Контактный телефон:</th>
            <th>Почта:</th>
            <th>Список услуг:</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.Client_Name</td>
            <td>@Model.Client_Contact_Person</td>
            <td>@Model.Client_Contact_Phone</td>
            <td>@Model.Client_Contact_Email</td>
            <td>
                <ul>
                    @foreach (var i in Model.Services)
                    {
                        <li>@i.Service_Name</li>
                    }
                </ul>
            </td>
        </tr>
    </tbody>
</table>