@model TaskAdminApi.Models.Service
@using TaskAdminApi.Models
@{
    ViewData["Title"] = "ClientDelete";
}


<div>
    <dl class="dl-horizontal">
        <dt>Наименование удаляемой услуги:</dt>
        <dd>
            @Html.DisplayFor(model => model.Service_Name)
        </dd>


    </dl>
</div>

<div>
    <form asp-controller="Service" asp-action="ServiceDelete" method="post">
        <div class="form-group">
            <input type="submit" value="Удалить" />
        </div>
    </form>
</div>
